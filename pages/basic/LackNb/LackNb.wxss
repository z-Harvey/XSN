.lack{
  width:100%;
  height:100%;
  position: fixed;
  top: 0rpx;
  left:0rpx;
  background: rgba(16,16,16,.6);
}
.MotBox{
  width:690rpx;
  height:660rpx;
  background:#fff;
  position: fixed;
  top:20%;
  left:30rpx;
  border-radius: 8rpx;
}
.title{
  width: 100%;
  border-radius: 8rpx 8rpx 0 0;
  height:124rpx;
  background:rgba(255, 172, 55, 1);
  text-align: center;
  font-size: 36rpx;
  color:#fff;
  line-height: 124rpx;
}
.contentBox{
  margin:20rpx 0;  
  border:1rpx solid rgba(0,0,0,0);
}
.contentBox>button{
  width:160rpx;
  height:60rpx;
  border:1rpx solid rgba(255, 152, 0, 1);
  border-radius: 40rpx;
  font-size: 28rpx;
  line-height:58rpx;
  background: #fff;
  color:rgba(255, 152, 0, 1);
  margin:10rpx auto;
}
.contentBox>view{
  margin:15rpx auto;
}
.contentBox>button::after{
  color:rgba(255, 152, 0, 1);
  border:0;
}
.contentBoxTit{
  text-align: center;
  color:rgba(16, 16, 16, 1);
  font-size: 28rpx;
  font-weight:bolder;
}
.contentBoxTit2>image{
  width:40rpx;
  height:40rpx;
  line-height: 20rpx;
}
.contentBoxTit2{
  font-size: 28rpx;
  line-height: 40rpx;
  text-align: center;
}
.footer{
  font-size: 28rpx; 
  height:100rpx;
  line-height: 100rpx;
  text-align: center;
  color:#FF9800;
  position: absolute;
  bottom: 0rpx;
  width:100%;
  font-weight:bolder;  
}
.footer>view>view{
  display: inline-block;
}
.footer>view{
  width:260rpx;
  margin:0 auto;
  text-align: left;
  position: relative;
}
.footer>view>image{
  width:24rpx;
  height:24rpx;
  position: absolute;
  top:38rpx;
  right:20rpx;
}
.cloase{
  position: absolute;
  top:-76rpx;
  right:54rpx;
  z-index: 9
}
.cloase>image{
  position: absolute;
  top:0rpx;
  width:58rpx;
  height:58rpx;
}
.gun{
  position: absolute;
  top:54rpx;
  left: 29rpx;
  width:2rpx;
  height:20rpx;
  background:#888888 100%;
  border:1rpx solid #888;
}
.btnBox{
  height:436rpx;
}